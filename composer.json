{
    "name": "xetaio/xetaravel",
    "description": "A light version of Xeta's website using Laravel.",
    "keywords": [
        "Xeta",
        "framework",
        "laravel",
        "website",
        "blog",
        "administration"
    ],
    "homepage": "https://github.com/XetaIO/Xetaravel",
    "license": "MIT",
    "type": "project",
    "support": {
        "source": "https://github.com/XetaIO/Xetaravel",
        "issues": "https://github.com/XetaIO/Xetaravel/issues"
    },
    "authors": [
        {
            "name": "Xety",
            "email": "zoro.fmt@gmail.com",
            "homepage": "https://github.com/Xety"
        }
    ],
    "require": {
        "php": "^8.0",
        "anhskohbo/no-captcha": "^3.0",
        "fideloper/proxy": "^4.3",
        "graham-campbell/markdown": "^13.0",
        "kirkbushell/eloquence": "^8.0",
        "laravel/framework": "^8.0",
        "laravel/socialite": "^5.0",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^3.0",
        "laravelcollective/html": "^6.0",
        "rinvex/countries": "^8.0",
        "spatie/laravel-analytics": "^4.0",
        "spatie/laravel-medialibrary": "^9.0",
        "spatie/laravel-menu": "^3.0",
        "xetaio/color-extractor": "^0.3.3",
        "xetaio/roles": "^8.0",
        "xetaio/xetaravel-editor-md": "^2.0",
        "xetaio/xetaravel-iptraceable": "^2.0",
        "xetaio/xetaravel-mentions": "^8.0",
        "xety/breadcrumbs": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.1",
        "filp/whoops": "^2.0",
        "laravel/homestead": "^13.2",
        "phpro/grumphp": "^1.0",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.2",
        "symfony/css-selector": "^4.0",
        "symfony/dom-crawler": "^4.0",
        "symfony/filesystem": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Xetaravel\\": "app/",
            "XetaravelInstaller\\": "install/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate",
            "npm install",
            "npm run development",
            "XetaravelInstaller\\Installer::postInstall"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "process-timeout": 2000,
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "kylekatarnls/update-helper": true,
            "phpro/grumphp": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
